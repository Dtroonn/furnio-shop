{
  "extends": "./common-tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "target": "ESNext",
    "noEmit": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "paths": {
      "components" : ["components"],
      "redux": ["redux"],
      "services": ["services"],
      "pages" : ["pages"],
      "hooks": ["hooks"],
      "styles": ["styles"],
      "assets": ["assets"],
      "common": ["common"],
    },
    "jsx": "react-jsx",
  },
  "include": [
    "src"
  ]
}
